<data-extraction-rules>
    <cloud-backup>
        <!-- Kuluçka ayarları ve tercihlerini yedekle -->
        <include domain="sharedpref" path="KuluckaMKv5Prefs.xml"/>
        <include domain="database" path="."/>
        <include domain="file" path="."/>
        <exclude domain="file" path="cache/"/>
    </cloud-backup>
